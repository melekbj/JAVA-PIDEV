<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/19" fx:controller="GUI.Controller.FormulaireproduitController">
    <stylesheets>
        <URL value="@/GUI/store.css" />
    </stylesheets>
   <children>
      <TextField fx:id="quantite" layoutX="226.0" layoutY="200.0" promptText="quantite" />
      <TextField fx:id="price" layoutX="226.0" layoutY="137.0" promptText="suggested price" />
      <TextField fx:id="productname" layoutX="226.0" layoutY="78.0" promptText="Product name" />
      <TextField fx:id="etat" layoutX="226.0" layoutY="264.0" promptText="etat" />
      <SplitMenuButton fx:id="choixCategorie" layoutX="375.0" layoutY="78.0" mnemonicParsing="false" text="Categorie">
        <items>
          <MenuItem mnemonicParsing="false" text="Action 1" />
          <MenuItem mnemonicParsing="false" text="Action 2" />
        </items>
      </SplitMenuButton>
      <Text layoutX="188.0" layoutY="42.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Add a product to your Store">
         <font>
            <Font size="18.0" />
         </font>
      </Text>
      <Button layoutX="225.0" layoutY="323.0" mnemonicParsing="false" onAction="#ajoutproduit" prefHeight="25.0" prefWidth="149.0" text="Confirme" />

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.Controller.FormulaireStoreController">
    <stylesheets>
        <URL value="@../store.css" />
    </stylesheets>
   <children>
      <TextField fx:id="STName" layoutX="78.0" layoutY="118.0" promptText="Name Store" />
      <TextField fx:id="STAdresse" layoutX="78.0" layoutY="259.0" promptText="Store Adresse" />
      <Button layoutX="226.0" layoutY="340.0" mnemonicParsing="false" onAction="#createStore" prefHeight="25.0" prefWidth="149.0" text="confirmer" />

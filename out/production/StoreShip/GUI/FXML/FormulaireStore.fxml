            <Font size="18.0" />
         </font>
      </Text>
      <ImageView fx:id="image_St" fitHeight="142.0" fitWidth="226.0" layoutX="312.0" layoutY="141.0" pickOnBounds="true" preserveRatio="true" />
      <Button layoutX="358.0" layoutY="105.0" mnemonicParsing="false" onAction="#insertimage" prefHeight="25.0" prefWidth="118.0" text="insert image" />
      <SplitMenuButton layoutX="78.0" layoutY="188.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="149.0" text="SplitMenuButton">
